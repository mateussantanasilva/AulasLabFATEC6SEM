<div class="container">
   <div class="row">
    <div class="col">
      <nav class="navbar navbar-expand-lg bg-primary" data-bs-theme="dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="#">SISTEMA WEB</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">
              <a class="nav-link" href="/">Cadastrar</a>
              <a class="nav-link active" aria-current="page" href="/consultar">Consultar</a>
            </div>
          </div>
        </div>
      </nav>
    </div>
  </div>

    <div class="row">
        <div class="col">
            &nbsp;
        </div>
    </div>
    
    <div class="row">
        <div class="col bg-white">
          <p class="text-start fs-5">Consultar - Agendamento de Potenciais Clientes</p>
          <p class="text-start fs-6">Sistema utilizado para agendamentos de serviços.</p>

            <div class="container text-center">
                <div class="row bg-primary">
                    <div class="col text-light">
                        Nome
                    </div>
                    <div class="col text-light">
                        Endereço
                    </div>
                    <div class="col text-light">
                        Bairro
                    </div>
                    <div class="col text-light">
                        Ações
                    </div>
                </div>
                <div class="row">
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                </div>
                {{#each appointments}}
                  {{#dataValues}}
                    <div class="row text-dark">
                        <div class="col">
                            {{nome}}
                        </div>
                        <div class="col">
                            {{endereco}}
                        </div>
                        <div class="col">
                            {{bairro}}
                        </div>
                        <div class="col">
                            <a href="/editar/{{id}}" class="btn btn-info">Editar</a>
                            <a href="/excluir/{{id}}" class="btn btn-danger">Excluir</a>
                        </div>
                    </div>
                    <div class="row">
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                </div>
                  {{/dataValues}}
                {{/each}}
            </div>
        </div>
    </div> 
</div>